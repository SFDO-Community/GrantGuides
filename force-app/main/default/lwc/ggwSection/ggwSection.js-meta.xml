<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>60.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>Ggw section</masterLabel>
	<targets>
		<target>lightning__AppPage</target>
		<target>lightning__HomePage</target>
		<target>lightning__FlowScreen</target>
		<target>lightning__Tab</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__AppPage">
			<supportedFormFactors>
				<supportedFormFactor type="Small"/>
				<supportedFormFactor type="Large"/>
			</supportedFormFactors>
		</targetConfig>
		<targetConfig targets="lightning__HomePage">
			<supportedFormFactors>
				<supportedFormFactor type="Large"/>
			</supportedFormFactors>
		</targetConfig>
		<targetConfig targets="lightning__FlowScreen">
            <property name="sectionTitle" label="Section Title" type="String"/>
            <property name="textBlock" label="Text" type="String"/>
			<property name="sectionId" label="Section Id" type="String"/>
			<property name="sectionHasBlocks" label="Has blocks" type="Boolean"/>
			<property name="applicationId" label="Application Id" type="String"/>
            <property name="displayTitle" label="Display Title" type="String"/>
        </targetConfig>	
	</targetConfigs>
</LightningComponentBundle>